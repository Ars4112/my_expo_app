{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "ESNext",
    "isolatedModules": true,
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "skipLibCheck": true,
    "strictNullChecks": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "target": "ES2022"
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "extends": "expo/tsconfig.base"
}
